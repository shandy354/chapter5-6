<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
    <%- include('../partials/header'); %>
</header>

<table class="table table-hover">
    <a href="/view/history/create" class="btn btn-primary px-3 py-1">Add Data</a>
    <tr class="text-center">
        <th>Id</th>
        <th>Diamond</th>
        <th>Tiket</th>
        <th>Battle Point</th>
        <th>Item</th>
        <th>Login</th>
        <th>Logout</th>
        <th>User Id</th>
        <th>Action</th>
    </tr>
        <% for(var i=0; i<history.length; i++) {%>
            <tr class="text-center">
                <td><%= history[i].id %></td>
                <td><%= history[i].diamon %></td>
                <td><%= history[i].tiket %></td>
                <td><%= history[i].battle_point %></td>
                <td><%= history[i].item %></td>
                <td><%= history[i].login %></td>
                <td><%= history[i].logout %></td>
                <td><%= history[i].userId %></td>
                <td class="text-center">
                    <a href="/view/history/update/<%= history[i].id %>" class="btn btn-primary px-3 py-1">Edit</a>
                    <a href="/view/history/delete/<%= history[i].id %>" class="text-danger  px-3 py-1 text-decoration-none">Delete</a>
                </td>
            </tr>
            <% } %>
  </table>

<footer>
    <%- include('../partials/footer'); %>
</footer>

</body>
</html>